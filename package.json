{
  "name": "@whoj/nuxt3-axios",
  "version": "1.4.1",
  "description": "An alternative to @nuxtjs/axios for Nuxt 3",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./*": [
      "./dist/*",
      "./dist/*.d.ts"
    ]
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "typesVersions": {
    "<=4.9": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "prepublishOnly": "run-s clear lint build",
    "release": "pnpx changelogen@latest --release",
    "test": "vitest",
    "clear": "rm -rf dist",
    "cleanup": "nuxi cleanup playground",
    "stub": "run-s cleanup dev:prepare",
    "lint": "eslint \"{**/*,*}.{ts,tsx,vue}\" --fix",
    "dev:prepare": "nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.0",
    "@vueuse/integrations": "^10.4.1",
    "@whoj/utils-core": "^1.7.5",
    "axios": "^1.5.0",
    "axios-retry": "^3.7.0",
    "defu": "^6.1.2"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/test-utils": "3.7.0",
    "@types/node": "^18.17.14",
    "@vueuse/core": "^10.4.1",
    "@whoj/eslint-config": "^1.4.2",
    "bumpp": "^9.2.0",
    "core-js": "^3.32.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.7.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.33.0",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "@nuxt/schema": "^3.6",
    "@vueuse/core": "^^10"
  },
  "pnpm": {
    "overrides": {
      "typescript": "$typescript"
    }
  },
  "packageManager": "pnpm@8.7.1",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/who-jonson/nuxt3-axios/issues"
  },
  "homepage": "https://github.com/who-jonson/nuxt3-axios#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/who-jonson/nuxt3-axios.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
